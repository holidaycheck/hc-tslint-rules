{
  "name": "uf-tslint-rules",
  "version": "0.0.1",
  "description": "Handmade tslint rules for the userflow project",
  "scripts": {
    "compile": "tsc -p ./",
    "test": "tslint --rules-dir ./dist/rules --test ./test/rules/*",
    "precommit": "lint-staged",
    "postinstall": "tsc"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "8.0.26",
    "husky": "0.14.3",
    "lint-staged": "4.0.4",
    "prettier": "1.6.1",
    "typescript": "2.5.2",
    "tslint": "5.7.0"
  },
  "peerDependencies": {
    "tslint": "5.7.0",
    "typescript": ">=2.5.2"
  },
  "lint-staged": {
    "*.+(ts|tsx)": ["prettier --single-quote --write", "git add"]
  }
}
